{
	"extends": "wikimedia",
	"plugins": [
		"mediawiki"
	],
	"env": {
		"browser": true,
		"jquery": true,
		"commonjs": true
	},
	"globals": {
		"dataValues": false,
		"util": false
	},
	"rules": {
		"mediawiki/valid-package-file-require": "error",
		"computed-property-spacing": "off",
		"indent": "off",
		"keyword-spacing": "off",
		"no-underscore-dangle": "off",
		"no-unused-vars": "off",
		"one-var": "off",
		"operator-linebreak": "off",
		"quote-props": "off",
		"space-before-function-paren": "off",
		"space-infix-ops": "off",
		"valid-jsdoc": "off",
		"vars-on-top": "off"
	}
}
